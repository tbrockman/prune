{
  "name": "prune-plasmo",
  "displayName": "prune",
  "version": "3.4.4",
  "description": "An extension to help you trim your garden of tabs.",
  "author": "theodore brockman",
  "license": "GPLv3",
  "bugs": {
    "url": "https://github.com/tbrockman/prune/issues"
  },
  "homepage": "https://github.com/tbrockman/prune#readme",
  "packageManager": "pnpm@8.6.0",
  "scripts": {
    "dev": "plasmo dev",
    "clean": "rm -fr build",
    "build": "npm-run-all build:chrome build:firefox build:edge",
    "build:chrome": "plasmo build --target=chrome-mv3 --zip --hoist",
    "build:firefox": "plasmo build --target=firefox-mv3 --zip --hoist",
    "build:edge": "plasmo build --target=edge-mv3 --zip --hoist",
    "build:safari": "",
    "test": "pnpm test:unit && pnpm test:e2e",
    "test:unit": "mocha tests/unit/**/*",
    "test:e2e": "npx playwright test"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.15",
    "@mui/lab": "^5.0.0-alpha.150",
    "@mui/material": "^5.14.15",
    "@plasmohq/messaging": "^0.5.0",
    "@plasmohq/storage": "^1.9.0",
    "plasmo": "0.84.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "zustand": "^4.4.4"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "4.0.1",
    "@playwright/test": "^1.41.2",
    "@types/chai": "^4.3.9",
    "@types/chrome": "0.0.248",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.8.9",
    "@types/react": "18.2.31",
    "@types/react-dom": "18.2.14",
    "chai": "^4.3.8",
    "esm": "^3.2.25",
    "mocha": "^8.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "3.0.3",
    "sinon": "^9.2.4",
    "sinon-chrome": "^3.0.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.0",
    "typescript": "5.0.4"
  },
  "manifest": {
    "permissions": [
      "alarms",
      "tabs",
      "storage",
      "bookmarks",
      "tabGroups"
    ],
    "browser_specific_settings": {
      "gecko": {
        "id": "iam@theo.lol"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tbrockman/prune.git"
  },
  "keywords": [
    "prune",
    "tabs",
    "browser",
    "extension"
  ]
}
