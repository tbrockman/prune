{
  "name": "prune-plasmo",
  "displayName": "Prune plasmo",
  "version": "0.0.0",
  "description": "A basic Plasmo extension.",
  "author": "theodorebrockman",
  "packageManager": "pnpm@6.11.0",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "build:firefox": "plasmo build --target=firefox-mv3",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register -r esm tests/**/*"
  },
  "dependencies": {
    "@emotion/cache": "^11.10.7",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.16",
    "@mui/lab": "^5.0.0-alpha.126",
    "@mui/material": "^5.12.0",
    "@plasmohq/messaging": "^0.2.1",
    "@plasmohq/storage": "^1.5.0",
    "plasmo": "0.68.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "zustand": "^4.3.7"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.5.2",
    "@types/chai": "^4.3.4",
    "@types/chrome": "0.0.197",
    "@types/node": "18.8.3",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "chai": "^4.3.7",
    "esm": "^3.2.25",
    "mocha": "^8.3.0",
    "prettier": "2.7.1",
    "sinon": "^9.2.4",
    "sinon-chrome": "^3.0.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.0.4"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "alarms",
      "tabs",
      "storage",
      "tabGroups",
      "bookmarks"
    ]
  }
}
