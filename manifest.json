{
	"name": "prune",
	"author": "Theodore Brockman",
	"version": "3.2.1",
	"description": "An extension to help you trim your garden of tabs.",
	"permissions": ["alarms", "tabs", "storage", "tabGroups", "bookmarks"],
	"background": {
		"service_worker": "service-worker.js"
	},
	"content_scripts": [
		{
			"matches": ["*"],
			"js": ["/dist/content-script.js"]
		}
	],
	"manifest_version": 3,
	"action": {
		"default_popup": "/dist/index.html",
		"default_title": "prune - options",
		"default_icon": "/assets/png/prune-icon128.png"
	},
	"icons": {
		"16": "/assets/png/prune-icon16.png",
		"48": "/assets/png/prune-icon48.png",
		"128": "/assets/png/prune-icon128.png"
	},
	"web_accessible_resources": [
		{
			"resources": ["test1.png", "test2.png"],
			"matches": ["https://web-accessible-resources-1.glitch.me/*"]
		}
	]
}
