{
    "name": "prune",
    "author": "Theodore Brockman",
    "version": "2.0.1",
    "description": "An extension to help you trim your garden of tabs.",
    "permissions": ["alarms", "tabs", "storage", "tabGroups"],
    "background": {
        "service_worker": "service-worker.js"
    },
    "manifest_version": 3,
    "options_page": "/src/options/index.html",
    "icons": {
        "16": "/assets/png/prune-icon16.png",
        "48": "/assets/png/prune-icon48.png",
        "128": "/assets/png/prune-icon128.png"
    }
}